var ('ss_lambda_B2')
B2=(ss_lambda_B2*(ss_Q*(ss_c2*exp(c2_))^(-sigma)*exp(Q_)-beta*(ss_c2*exp(c2))^(-sigma)))/(beta*ss_c2^(-sigma))
