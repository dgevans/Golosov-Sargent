var ('ss_lambda_R')
R=ss_lambda_R*(ss_c1*exp(c1)+ss_c2*exp(c2)-ss_theta_1*exp(theta_1)*ss_l1*exp(l1)-ss_theta_2*exp(theta_2)*ss_l2*exp(l2)+ss_g*exp(g))
SS_R=R(ss_lambda_R=1,c1_=0,c2_=0,l1_=0,l2_=0,b2_=0,Q_=0,c1=0,c2=0,l1=0,l2=0,b2=0,Q=0,g=0,theta_1=0,theta_2=0)
RLinearApprox= simplify((taylor(R,(c1_,0),(c2_,0),(l1_,0),(l2_,0),(b2_,0),(Q_,0),(c1,0),(c2,0),(l1,0),(l2,0),(b2,0),(Q,0),(g,0),(theta_1,0),(theta_2,0),1)))
RQuadApprox= simplify((taylor(R,(c1_,0),(c2_,0),(l1_,0),(l2_,0),(b2_,0),(Q_,0),(c1,0),(c2,0),(l1,0),(l2,0),(b2,0),(Q,0),(g,0),(theta_1,0),(theta_2,0),2)))-RLinearApprox
# Linear Terms
RDy = vector([ RLinearApprox.coefficient(c1) , RLinearApprox.coefficient(c2) , RLinearApprox.coefficient(l1) , RLinearApprox.coefficient(l2) , RLinearApprox.coefficient(b2) , RLinearApprox.coefficient(Q)])
