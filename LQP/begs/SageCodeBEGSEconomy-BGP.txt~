
var ('sigma')
var ('psi')
var ('beta')
var ('alpha_1')
var ('alpha_2')
var ('ss_c1')
var ('ss_c2')
var ('ss_l1')
var ('ss_l2')
var ('ss_b2')
var ('ss_Q')
var ('ss_g')
var ('ss_theta_1')
var ('ss_theta_2')
