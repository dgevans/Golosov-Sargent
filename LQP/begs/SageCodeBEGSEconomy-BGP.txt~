var ('ss_lambda_B1')
B1=(ss_lambda_B1*(ss_Q*(ss_c1*exp(c1_))^(-sigma)*exp(Q_)-beta*(ss_c1*exp(c1))^(-sigma)))/(beta*ss_c1^(-sigma))
SS_B1=B1(ss_lambda_B1=1,c1_=0,c2_=0,l1_=0,l2_=0,b2_=0,Q_=0,c1=0,c2=0,l1=0,l2=0,b2=0,Q=0,g=0,theta_1=0,theta_2=0)
B1LinearApprox= simplify((taylor(B1,(c1_,0),(c2_,0),(l1_,0),(l2_,0),(b2_,0),(Q_,0),(c1,0),(c2,0),(l1,0),(l2,0),(b2,0),(Q,0),(g,0),(theta_1,0),(theta_2,0),1)))
