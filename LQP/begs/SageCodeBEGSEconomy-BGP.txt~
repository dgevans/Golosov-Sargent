var ('ss_lambda_W')
W=ss_lambda_W*(ss_l1*exp(l1+sigma*c1)*ss_c1^(sigma)/(1-ss_l1*exp(l1))-ss_l2*exp(l2+sigma*c2)*ss_c2^(sigma)/(1-ss_l2*exp(l2)))
