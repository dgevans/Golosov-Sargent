var ('l2')
var ('b2')
var ('Q')
var ('g')
var ('theta_1')
var ('theta_2')
var ('ss_lambda_I')
I=ss_lambda_I*(( ss_c2*exp(c2)- ss_c1*exp(c1)) + ss_Q*ss_b2*exp(b2+Q)+ ((ss_c1*exp(c1))^(sigma)/psi)*((1-psi)*ss_l1*exp(l1)/(1-ss_l1*exp(l1)))-((ss_c2*exp(c2))^(sigma)/psi)*((1-psi)*ss_l2*exp(l2)/(1-ss_l2*exp(l2))) -ss_b2*exp(b2_))
SS_I=I(ss_lambda_I=1,c1_=0,c2_=0,l1_=0,l2_=0,b2_=0,Q_=0,c1=0,c2=0,l1=0,l2=0,b2=0,Q=0,g=0,theta_1=0,theta_2=0)
ILinearApprox= simplify((taylor(I,(c1_,0),(c2_,0),(l1_,0),(l2_,0),(b2_,0),(Q_,0),(c1,0),(c2,0),(l1,0),(l2,0),(b2,0),(Q,0),(g,0),(theta_1,0),(theta_2,0),1)))
IQuadApprox= simplify((taylor(I,(c1_,0),(c2_,0),(l1_,0),(l2_,0),(b2_,0),(Q_,0),(c1,0),(c2,0),(l1,0),(l2,0),(b2,0),(Q,0),(g,0),(theta_1,0),(theta_2,0),2)))-ILinearApprox
# Linear Terms
IDy = vector([ ILinearApprox.coefficient(c1) , ILinearApprox.coefficient(c2) , ILinearApprox.coefficient(l1) , ILinearApprox.coefficient(l2) , ILinearApprox.coefficient(b2) , ILinearApprox.coefficient(Q)])
IDycheck = vector([ ILinearApprox.coefficient(c1_) , ILinearApprox.coefficient(c2_) , ILinearApprox.coefficient(l1_) , ILinearApprox.coefficient(l2_) , ILinearApprox.coefficient(b2_) , ILinearApprox.coefficient(Q_)])
IDxi = vector([ ILinearApprox.coefficient(g) , ILinearApprox.coefficient(theta_1) , ILinearApprox.coefficient(theta_2)])
# Quadratic Terms
IDyy = matrix([[ IQuadApprox.coefficient(c1^2), (IQuadApprox.coefficient(c1)/2).coefficient(c2), (IQuadApprox.coefficient(c1)/2).coefficient(l1), (IQuadApprox.coefficient(c1)/2).coefficient(l2), (IQuadApprox.coefficient(c1)/2).coefficient(b2), (IQuadApprox.coefficient(c1)/2).coefficient(Q)], [ (IQuadApprox.coefficient(c2)/2).coefficient(c1), IQuadApprox.coefficient(c2^2), (IQuadApprox.coefficient(c2)/2).coefficient(l1), (IQuadApprox.coefficient(c2)/2).coefficient(l2), (IQuadApprox.coefficient(c2)/2).coefficient(b2), (IQuadApprox.coefficient(c2)/2).coefficient(Q)], [ (IQuadApprox.coefficient(l1)/2).coefficient(c1), (IQuadApprox.coefficient(l1)/2).coefficient(c2), IQuadApprox.coefficient(l1^2), (IQuadApprox.coefficient(l1)/2).coefficient(l2), (IQuadApprox.coefficient(l1)/2).coefficient(b2), (IQuadApprox.coefficient(l1)/2).coefficient(Q)], [ (IQuadApprox.coefficient(l2)/2).coefficient(c1), (IQuadApprox.coefficient(l2)/2).coefficient(c2), (IQuadApprox.coefficient(l2)/2).coefficient(l1), IQuadApprox.coefficient(l2^2), (IQuadApprox.coefficient(l2)/2).coefficient(b2), (IQuadApprox.coefficient(l2)/2).coefficient(Q)], [ (IQuadApprox.coefficient(b2)/2).coefficient(c1), (IQuadApprox.coefficient(b2)/2).coefficient(c2), (IQuadApprox.coefficient(b2)/2).coefficient(l1), (IQuadApprox.coefficient(b2)/2).coefficient(l2), IQuadApprox.coefficient(b2^2), (IQuadApprox.coefficient(b2)/2).coefficient(Q)], [ (IQuadApprox.coefficient(Q)/2).coefficient(c1), (IQuadApprox.coefficient(Q)/2).coefficient(c2), (IQuadApprox.coefficient(Q)/2).coefficient(l1), (IQuadApprox.coefficient(Q)/2).coefficient(l2), (IQuadApprox.coefficient(Q)/2).coefficient(b2), IQuadApprox.coefficient(Q^2)] ]) 
IDycheckycheck = matrix([[ IQuadApprox.coefficient(c1_^2), (IQuadApprox.coefficient(c1_)/2).coefficient(c2_), (IQuadApprox.coefficient(c1_)/2).coefficient(l1_), (IQuadApprox.coefficient(c1_)/2).coefficient(l2_), (IQuadApprox.coefficient(c1_)/2).coefficient(b2_), (IQuadApprox.coefficient(c1_)/2).coefficient(Q_)], [ (IQuadApprox.coefficient(c2_)/2).coefficient(c1_), IQuadApprox.coefficient(c2_^2), (IQuadApprox.coefficient(c2_)/2).coefficient(l1_), (IQuadApprox.coefficient(c2_)/2).coefficient(l2_), (IQuadApprox.coefficient(c2_)/2).coefficient(b2_), (IQuadApprox.coefficient(c2_)/2).coefficient(Q_)], [ (IQuadApprox.coefficient(l1_)/2).coefficient(c1_), (IQuadApprox.coefficient(l1_)/2).coefficient(c2_), IQuadApprox.coefficient(l1_^2), (IQuadApprox.coefficient(l1_)/2).coefficient(l2_), (IQuadApprox.coefficient(l1_)/2).coefficient(b2_), (IQuadApprox.coefficient(l1_)/2).coefficient(Q_)], [ (IQuadApprox.coefficient(l2_)/2).coefficient(c1_), (IQuadApprox.coefficient(l2_)/2).coefficient(c2_), (IQuadApprox.coefficient(l2_)/2).coefficient(l1_), IQuadApprox.coefficient(l2_^2), (IQuadApprox.coefficient(l2_)/2).coefficient(b2_), (IQuadApprox.coefficient(l2_)/2).coefficient(Q_)], [ (IQuadApprox.coefficient(b2_)/2).coefficient(c1_), (IQuadApprox.coefficient(b2_)/2).coefficient(c2_), (IQuadApprox.coefficient(b2_)/2).coefficient(l1_), (IQuadApprox.coefficient(b2_)/2).coefficient(l2_), IQuadApprox.coefficient(b2_^2), (IQuadApprox.coefficient(b2_)/2).coefficient(Q_)], [ (IQuadApprox.coefficient(Q_)/2).coefficient(c1_), (IQuadApprox.coefficient(Q_)/2).coefficient(c2_), (IQuadApprox.coefficient(Q_)/2).coefficient(l1_), (IQuadApprox.coefficient(Q_)/2).coefficient(l2_), (IQuadApprox.coefficient(Q_)/2).coefficient(b2_), IQuadApprox.coefficient(Q_^2)] ]) 
